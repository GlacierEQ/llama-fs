// For format details, see https://aka.ms/devcontainer.json. For config options, see the
// README at: https://github.com/devcontainers/templates/tree/main/src/python
{
	"name": "Python 3",
	// Or use a Dockerfile or Docker Compose file. More info: https://containers.dev/guide/dockerfile
	"image": "mcr.microsoft.com/devcontainers/python:1-3.12-bullseye",
	"features": {
		"ghcr.io/devcontainers/features/powershell:1": {
			"version": "latest"
		},
		"ghcr.io/devcontainers/features/terraform:1": {
			"installSentinel": true,
			"installTFsec": true,
			"installTerraformDocs": true,
			"version": "latest",
			"tflint": "latest",
			"terragrunt": "latest"
		},
		"ghcr.io/KusionStack/devcontainer-features/kusion:0": {},
		"ghcr.io/warrenbuckley/codespace-features/sqlite:1": {},
		"ghcr.io/michidk/devcontainers-features/typos:1": {},
		"ghcr.io/wxw-matt/devcontainer-features/command_runner:0": {},
		"ghcr.io/wxw-matt/devcontainer-features/script_runner:0": {},
		"ghcr.io/codeman99/features/circleci-cli:1": {
			"completions": true,
			"telemetry": "disable"
		},
		"ghcr.io/joshuanianji/devcontainer-features/edgedb-cli:1": {},
		"ghcr.io/nullcoder/devcontainer-features/stripe-cli:1": {
			"version": "latest"
		},
		"ghcr.io/itsmechlark/features/1password:1": {
			"version": "latest"
		},
		"ghcr.io/itsmechlark/features/act:1": {
			"version": "latest"
		},
		"ghcr.io/hspaans/devcontainer-features/sshpass:1": {
			"installDirectlyFromGitHubRelease": true,
			"version": "latest"
		},
		"ghcr.io/bartventer/arch-devcontainer-features/terraform:1": {
			"enableShellCompletion": true,
			"installTerragrunt": true,
			"installSentinel": true,
			"installTFsec": true,
			"installTerraformDocs": true,
			"tflint": "latest"
		},
		"ghcr.io/gvatsal60/dev-container-features/pre-commit:1": {},
		"ghcr.io/devcontainers-extra/features/tooljet-cli:1": {
			"version": "latest"
		},
		"ghcr.io/devcontainers-extra/features/turborepo-npm:1": {
			"version": "latest"
		},
		"ghcr.io/devcontainers-extra/features/ufmt:1": {
			"version": "latest"
		},
		"ghcr.io/devcontainers-extra/features/vscode-cli:1": {
			"version": "latest"
		},
		"ghcr.io/devcontainers-extra/features/vscode-server:1": {
			"version": "latest"
		},
		"ghcr.io/devcontainers-extra/features/vulture:2": {
			"version": "latest"
		},
		"ghcr.io/devcontainers-extra/features/webi-packages:1": {},
		"ghcr.io/devcontainers-extra/features/webtau-sdkman:2": {
			"version": "latest",
			"jdkVersion": "latest",
			"jdkDistro": "ms"
		},
		"ghcr.io/devcontainers-extra/features/yamllint:2": {
			"version": "latest"
		}
	}

	// Features to add to the dev container. More info: https://containers.dev/features.
	// "features": {},

	// Use 'forwardPorts' to make a list of ports inside the container available locally.
	// "forwardPorts": [],

	// Use 'postCreateCommand' to run commands after the container is created.
	// "postCreateCommand": "pip3 install --user -r requirements.txt",

	// Configure tool-specific properties.
	// "customizations": {},

	// Uncomment to connect as root instead. More info: https://aka.ms/dev-containers-non-root.
	// "remoteUser": "root"
}
